    <div id="all">

        <div id="content">

            <div class="container">
            
            
 

                <div class="col-md-12" id="customer-orders">
                    <div class="box">
                        <h1>Member List</h1>

                        <p class="lead">Your members on one place.</p>
                       
                        <hr>

                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>email</th>
                                        <th>nickname</th>
                                        <th>pwd</th>
                                        <th>grade</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                
                                <c:forEach items="${objMList }" var="mdto">
                                    <tr>
                                        <th>${mdto.email }</th>
                                        <td>${mdto.nickname }</td>
                                        <td>${mdto.pwd }</td>
                                        <td>
                                        <c:choose>
											<c:when test="${mdto.grade eq '0' }"><span class="label label-info">일반회원</c:when>
											<c:when test="${mdto.grade eq '5' }"><span class="label label-info">우수회원</c:when>
											<c:when test="${mdto.grade eq '10' }"><span class="label label-info">골드회원</c:when>
											<c:when test="${mdto.grade eq '20' }"><span class="label label-info">VIP회원</c:when>
											<c:when test="${mdto.grade eq '99' }"><span class="label label-info">관리자</c:when>
										</c:choose>
                                        </td>
                                        
                                        
                                        
                                        <td><a href="customer-order.html" class="btn btn-primary btn-sm">Delete</a>
                                        </td>
                                    </tr>
                                </c:forEach>    
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
